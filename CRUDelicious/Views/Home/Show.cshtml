@model CRUDelicious.Models.Dish

<div class="wrapper">
    <div class="nav">
        <a href="/">Home</a>
    </div>
    <div class="title">
        <h1>@Model.Name</h1>
        <h3>@Model.Chef</h3>
    </div>
    <hr>
    <div class="contant>">
        <p><b>@Model.Description</b></p>
        <p>Calories: @Model.Calories kcal</p>
        <p>Tastiness: @Model.Tastiness</p>
    </div>
    <div class="button">
        <a href="delete/@Model.DishId"><button type="submit">Delete</button></a>
        <a href="edit/@Model.DishId"><button type="submit">Edit</button></a>
    </div>
</div>